{
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "no-useless-escape": 0,
      "babel/semi": 1
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8
    },
    "parser": "babel-eslint",
    "plugins": [
      "babel"
    ]
  }